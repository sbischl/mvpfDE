@article{doerrenberg2017,
  author={Doerrenberg, Philipp and Peichl, Andreas and Siegloch, Sebastian},
  title={{The elasticity of taxable income in the presence of deduction possibilities}},
  journal={Journal of Public Economics},
  year=2017,
  volume={151},
  pages={41-55},
  month={},
  keywords={Elasticity of taxable income; Deductions; Tax expenditures; Sufficient statistic; Administrative dat},
  doi={10.1016/j.jpubeco.2015.10},
  abstract={Several recent studies show that the elasticity of taxable income (ETI) is not a sufficient statistic for the welfare costs of taxation due to factors such as tax-base shifting. This paper provides an additional argument demonstrating the non-sufficiency of the ETI, namely tax deductions. Building on a theoretical framework which incorporates deductions in a standard optimal-tax model, we show that the ETI is not sufficient for welfare analysis if (i) deductions generate externalities and if (ii) deductions are responsive to tax-rate changes. While the first condition should arguably hold true for the majority of tax deductions, we provide an empirical examination of the second condition. Relying on rich German panel data from administrative tax records, we exploit several tax reforms that were implemented in Germany between 2001 and 2008. Our main estimates indicate an overall ETI between 0.54 and 0.68 and an elasticity of deductions with respect to the net-of-tax rate of about −0.9. These results suggest that the ETI is not sufficient to calculate the welfare cost of taxation.},
  url={https://ideas.repec.org/a/eee/pubeco/v151y2017icp41-55.html}
}

